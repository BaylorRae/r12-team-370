<!DOCTYPE html>
<html>
<head>
  <title>Churumelas</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="logo">
    <a href="/game/start"><%= image_tag "logo-medium.png" %></a>
  </div>
  <% flash.each do |key, value| %>
    <div class="flash <%= key %>"><%= value %></div>
  <% end %>
<%= yield %>
</body>
</html>
